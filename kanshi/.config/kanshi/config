profile docked {
  exec pkill -SIGUSR2 waybar
  output "LG Display 0x0521 Unknown" disable
  output "Acer Technologies XB271HU T4TAA0088502" enable mode 2560x1440@60Hz adaptive_sync on
}

profile laptop {
  exec pkill -SIGUSR2 waybar
  output "LG Display 0x0521 Unknown" enable mode 1920x1080 scale 1
}

